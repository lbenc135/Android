            android:name="com.licoforen.apitestproject.AndroidBasicsStarter"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="Life Cycle Test"
					android:name="com.licoforen.androidgames.LifeCycleTest"
					android:configChanges="keyboard|keyboardHidden|orientation" />
        <activity android:label="Single Touch Test"
					android:name="com.licoforen.androidgames.SingleTouchTest"
					android:configChanges="keyboard|keyboardHidden|orientation" />
        <activity android:label="Multi Touch Test"
					android:name="com.licoforen.androidgames.MultiTouchTest"
					android:configChanges="keyboard|keyboardHidden|orientation" />
        <activity android:label="Key Test"
					android:name="com.licoforen.androidgames.KeyTest"
					android:configChanges="keyboard|keyboardHidden|orientation" />
