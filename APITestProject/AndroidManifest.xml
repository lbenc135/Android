					android:name="com.licoforen.androidgames.AccelerometerTest"
					android:configChanges="keyboard|keyboardHidden|orientation" />
        <activity android:label="Assets Test"
					android:name="com.licoforen.androidgames.AssetsTest"
					android:configChanges="keyboard|keyboardHidden|orientation" />
